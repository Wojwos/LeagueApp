<UserControl x:Class="LeagueApp.Views.AccountStats"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LeagueApp.Views"
             mc:Ignorable="d" 
             d:DesignHeight="550" d:DesignWidth="750" Background="#FF864489">
    <Grid>
        <StackPanel Margin="100">
            <TextBox Width="200" Height="50" Text="{Binding SummonerTier}"/>
            <TextBox Width="200" Height="50" Text="{Binding SummonerRank}"/>
            <TextBox Width="200" Height="50" Text="{Binding SummonerWins}"/>
            <TextBox Width="200" Height="50" Text="{Binding SummonerLosses}"/>
            <TextBox Width="200" Height="50" Text="{Binding SummonerWinRatio}"/>
        </StackPanel>
    </Grid>
</UserControl>
